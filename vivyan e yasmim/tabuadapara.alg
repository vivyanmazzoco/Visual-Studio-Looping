algoritmo "tabuada para"
// Função :
// Autor :
// Data : 30/03/2023
// Seção de Declarações 
var
 numtabela,result,i:real
i:=1
inicio

 escreva("Digite o numero da tabuada: ")
 leia(numtabela)
repita
 result:=numtabela*i
 result:=numtabela+result
 i:=i+1
 escreval("O numero da tabuada é: ",result)

fimalgoritmo
